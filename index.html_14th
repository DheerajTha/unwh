  <style>
    .course-section {
      padding: 4rem 0;
      background: #f3f9ff;
    }

    .course-heading {
      color: white;
      font-size: 2.5rem;
      margin-bottom: 3rem;
      text-align: center;
    }

    .custom-container {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
      gap: 1rem;
      padding: 0 2rem;
      max-width: 1400px;
      margin: 0 auto;
    }

    .custom-card {
      position: relative;
      width: 100%;
      height: 750px;
      box-shadow: inset 5px 5px 5px rgba(0, 0, 0, 0.2),
        inset -5px -5px 15px rgba(255, 255, 255, 0.1),
        5px 5px 15px rgba(0, 0, 0, 0.3),
        -5px -5px 15px rgba(255, 255, 255, 0.1);
      border-radius: 15px;
      transition: transform 0.4s ease, box-shadow 0.4s ease;
    }

    .custom-box {
      position: absolute;
      top: 20px;
      left: 20px;
      right: 20px;
      bottom: 20px;
      background: #6aa8b8;
      border-radius: 15px;
      display: flex;
      flex-direction: column;
      overflow: hidden;
      transition: 0.5s;
    }

    .custom-box:hover {
      transform: translateY(-25px);
      box-shadow: inset 5px 5px 5px rgba(0, 0, 0, 0.2),
        inset -5px -5px 15px rgba(255, 255, 255, 0.1),
        10px 10px 25px rgba(0, 0, 0, 0.4),
        -10px -10px 15px rgba(255, 255, 255, 0.1);
    }

    .custom-box::before {
      content: "";
      position: absolute;
      top: 0;
      left: 0;
      width: 50%;
      height: 100%;
      background: #5f9faf;
    }

    .custom-content {
      padding: 20px;
      color: white;
      position: relative;
      z-index: 1;
      height: 100%;
      display: flex;
      flex-direction: column;
    }

    .scrollable-content {
      flex-grow: 1;
      overflow-y: auto;
      padding-right: 10px;
      margin-bottom: 15px;
    }

    .scrollable-content::-webkit-scrollbar {
      width: 5px;
    }

    .scrollable-content::-webkit-scrollbar-thumb {
      background: rgba(255, 255, 255, 0.829);
      border-radius: 3px;
    }

    .custom-content h2 {
      position: absolute;
      top: -10px;
      right: 30px;
      font-size: 6rem;
      color: rgba(255, 255, 255, 0.15);
      z-index: 0;
      font-weight: 800;
      margin: 0;
      pointer-events: none;
    }

    .custom-content h3 {
      font-size: 1.5rem;
      margin-bottom: 15px;
      color: white;
      position: relative;
      z-index: 2;
      text-align: center;
    }

    .card-image {
      width: 100%;
      height: 160px;
      object-fit: cover;
      border-radius: 10px;
      margin-bottom: 20px;
      transition: transform 0.3s ease;
    }

    .custom-card:hover .card-image {
      transform: scale(1.05);
    }

    .card-btn {
      display: inline-block;
      background-color: #d45a00;
      color: white;
      padding: 10px 20px;
      border-radius: 6px;
      font-weight: 600;
      text-decoration: none;
      transition: all 0.3s ease;
      position: relative;
      z-index: 2;
      border: none;
      cursor: pointer;
      margin-top: auto;
      align-self: center;
    }

    .card-btn:hover {
      background-color: #ff6b00;
      transform: translateY(-2px);
      box-shadow: 0 5px 15px rgba(212, 90, 0, 0.4);
    }

    .program-container {
      display: flex;
      flex-direction: column;
      gap: 1rem;
    }

    .department {
      margin-bottom: 1rem;
    }

    .department h4 {
      color: #ff6b00;
      font-size: 1rem;
      margin-bottom: 0.5rem;
      font-weight: 600;
    }

    .program-list {
      list-style-type: none;
      padding: 0;
      margin: 0;
    }

    .program-list li {
      font-size: 0.85rem;
      color: rgba(255, 255, 255, 0.9);
      line-height: 1.4;
      margin-bottom: 0.5rem;
      padding-left: 1rem;
      position: relative;
    }

    .program-list li:before {
      content: "•";
      color: #ff6b00;
      position: absolute;
      left: 0;
    }

    /* Responsive adjustments */
    @media (max-width: 1024px) {
      .custom-container {
        grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
      }

      .custom-card {
        height: 700px;
      }
    }

    @media (max-width: 768px) {
      .custom-container {
        grid-template-columns: 1fr;
        max-width: 500px;
      }

      .custom-card {
        height: auto;
        min-height: 700px;
      }

      .custom-content h2 {
        font-size: 5rem;
      }
    }

    @media (max-width: 480px) {
      .custom-content h3 {
        font-size: 1.3rem;
      }

      .card-btn {
        padding: 8px 16px;
      }
    }
  </style>
